{
  "name": "animate",
  "title": "Animate.js",
  "description": "This is javascript animate tools.",
  "version": "0.2.0",
  "main": "dist/animate.js",
  "homepage": "http://github.com/biossun/animate",
  "keywords": [ "animate" ],
  "author": {
    "name": "Bios Sun",
    "url": "http://biossun.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/biossun/animate"
  },
  "license": [
    {
      "type": "MIT",
      "url": "https://github.com/biossun/animate/blob/master/LICENSE.txt"
    }
  ],
  "bugs": {
    "url": "https://github.com/biossun/animate/issues"
  },
  "scripts": {
    "start": "uglifyjs animate.js --compress --mangle -o animate.min.js && uglifyjs easing.js --compress --mangle -o easing.min.js"
  },
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "browsers": ["ios >= 6", "android >= 4.1"]
        },
        "exclude": ["transform-es2015-typeof-symbol"]
      }]
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "uglify-js": "^2.7.4"
  }
}
